@model ToobarModel
<div class="row">
    <div class="col-md-4">
        <h6 class="page-title">@Model.Title</h6>
    </div>
    <div class="col-md-8">
        <div class="btn-toolbar d-flex justify-content-end">
            <div class="btn-group">
                <button type="button" class="btn btn-default success-btn js-department-save">
                    <i class="fa fa-save">&nbsp;</i> Save
                </button>
                <button type="button" class="btn btn-default danger-btn js-department-delete-confirm">
                    <i class="fa fa-trash">&nbsp;</i>
                    Delete
                </button>
                <button type="button" class="btn btn-default primary-btn js-department-clear">
                    <i class="fas fa-sync">&nbsp;</i>
                    Clear
                </button>
                <button type="button" id="exportExcel" class="btn btn-light btn-sm"><i class="fas fa-print"></i> Print</button>
            </div>
        </div>
    </div>
</div>


  
       
   
