@model ToobarModel
<div class="row">
    <div class="col-md-5">
        <h6>@Model.Title</h6>
    </div>
    <div class="col-md-7">
        <div class="btn-group" style="float:right">
            <button type="submit" id="submitSupplierBtn" class="btn btn-light btn-sm">
                <i class="far fa-save">Save</i>
            </button>
            <button type="button" id="deleteMultiple" class="btn btn-light btn-sm"><i class="far fa-trash-alt"></i> Delete</button>
            <button type="button" id="resetButton" class="btn btn-light btn-sm"><i class="fas fa-eraser"></i> Clear</button>
            <button type="button" class="btn btn-light btn-sm"><i class="fas fa-print"></i> Print</button>
            <button type="button" class="btn btn-light btn-sm"><i class="far fa-star"></i> Fav</button>
        </div>
    </div>
</div>


