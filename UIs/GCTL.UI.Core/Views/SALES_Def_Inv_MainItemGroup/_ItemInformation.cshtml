                        <div class="gap-1 d-flex jusify-content-center align-items-center input-group-container">
                            <div class="p-0 text-right input-group-container-label">Item Id</div>
                            <div class="p-0 input-group-container-input">
                                <input type="text" asp-for="MainItem.ItemID" readonly class="form-control form-control-sm" placeholder="Item ID" aria-label="MainItemID">
                            </div>
                        </div>
                    </div>

                    <!-- Item Name -->
