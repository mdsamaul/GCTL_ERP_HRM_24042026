                                    <div class="col-6" style=" padding-left: 2px;">
                                        <select class="form-select form-select-sm searchable-select" asp-items="ViewBag.UnitTypeList as SelectList" asp-for="MainItem.ItemUnit">
                                            <option value="">Select Unit</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
