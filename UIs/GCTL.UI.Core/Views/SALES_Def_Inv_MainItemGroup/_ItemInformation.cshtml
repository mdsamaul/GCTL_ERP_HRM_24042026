                        <div class="gap-1 d-flex jusify-content-center align-items-center input-group-container">
                            <div class="p-0 text-right input-group-container-label">Name <span class="text-danger ">*</span></div>
                            <div class="p-0 input-group-container-input">
                                <input type="text" asp-for="MainItem.ItemName" class="form-control form-control-sm" placeholder="Enter Product Name" aria-label="MainItemName">
                            </div>
                        </div>
                    </div>

                    <!-- Description -->
                    <div style="width: 33.33vw;">
                        <div class="form-floating d-flex justify-content-end align-items-center input-group-container" style="padding-right: 0px;">
                            <div class="text-right p-0 input-group-container-label">
                                <label for="floatingTextarea">Print Name</label>
                            </div>
                            <div class="input-group-container-input">
                                <textarea asp-for="MainItem.PrintName" class="form-control form-control-sm" readonly placeholder="Enter Product Description" rows="1" style="height: 45px;"></textarea>
