                            <div class="p-0 input-group-container-input">
                                <div class="row g-1">
                                    <div class="col-6" style="padding-right: 2px;">
                                        <input type="number" asp-for="MainItem.ItemPrice" class="form-control form-control-sm" placeholder="Enter Unit Price">
                                    </div>
                                    <div class="col-6" style="padding-left: 2px;">
