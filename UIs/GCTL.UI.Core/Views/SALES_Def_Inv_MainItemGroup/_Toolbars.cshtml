@model ToobarModel
<div class="row">
	<div class="col-md-5">
		<h6 class="page-title">Product / Item Setup : </h6>
	</div>
	<div class="col-md-7">
		<div class="btn-toolbar d-flex justify-content-end">
			<div class="btn-group">
				<button type="button" class="btn btn-default border-0 success-btn js-MainI-tem-Group-save" id="submitButton">
					<i class="fa fa-save">&nbsp;</i> Save
				</button>
				<button type="button" class="btn btn-default danger-btn" id="js-MainI-tem-Group-delete-confirm">
					<i class="fa fa-trash">&nbsp;</i> Delete
				</button>
				<button type="button" class="btn btn-default primary-btn " id="js-MainI-tem-Group-clear">
					<i class="fa fa-eraser">&nbsp;</i> Clear
				</button>
				<button type="button" class="btn btn-default info-btn js-MainI-tem-Group-print">
					<i class="fa fa-print">&nbsp;</i> Print
				</button>
				<button type="button" class="btn btn-default warning-btn js-MainI-tem-Group-favorite">
					<i class="fa fa-star">&nbsp;</i> Favorite
				</button>
			</div>
		</div>
	</div>
</div>