@model ToobarModel
<div class="row">
    <div class="col-md-5">
        <h6>@Model.Title</h6>
    </div>
    <div class="col-md-7">
        <div class="btn-group" style="float:right">
            <button type="button" id="btnSubmit" class="btn btn-outline-info js-save"><i class="far fa-save fa-sm pr-2"></i>Save</button>
            <button type="button" id="btnClear" data-id="" class="btn btn-outline-dark js-clear"><i class="fas fa-recycle fa-sm pr-2"></i>Clear</button>
            <button type="button" id="btnDelete" class="btn btn-outline-danger js-delete-confirm"
                    data-target="#deleteModal"
                    data-id="@Model.Code"
                    data-title="Are you sure want to delete @Model.Name?">
                <i class="far fa-minus-square fa-sm pr-2"></i>Delete
            </button>
        </div>
    </div>
</div>


