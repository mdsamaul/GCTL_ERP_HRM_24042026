@model ToobarModel
<div class="row">
    <div class="col-md-5">
        <h6 class="page-title">@Model.Title</h6>
    </div>
    <div class="col-md-7">
        <div class="btn-toolbar d-flex justify-content-end">
            <div class="btn-group">
                <button type="button" class="btn btn-default success-btn btn btn-outline-success js-FinancialYear-save">
                    <i class="fa fa-save">&nbsp;</i> Save
                </button>

                <button type="button" class="btn btn-default danger-btn js-FinancialYear-delete-confirm">
                    <i class="fa fa-trash">&nbsp;</i>
                    Delete
                </button>
                <button type="button" class="btn btn-default primary-btn js-FinancialYear-clear">
                    <i class="fas fa-sync">&nbsp;</i>
                    Clear
                </button>
                <button type="button" class="btn btn-default info-btn js-FinancialYear-info-dec-print">
                    <i class="fa fa-print">&nbsp;</i> Print
                </button>
                <button type="button" class="btn btn-default warning-btn js-FinancialYear-info-dec-favorite">
                    <i class="fa fa-star">&nbsp;</i> Favorite
                </button>
            </div>
        </div>
    </div>
</div>